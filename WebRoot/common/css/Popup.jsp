	height:18px;
}
/*开始功能按钮行*/
.Btn_all_normalbg
{
	background-repeat:repeat-x;
	border-right:2px inset #C4C2C2;
	border-bottom:2px inset #C4C2C2;
	height:34px;
}
.Btn_chosen_bg
{
	background-repeat:repeat-x;
	border-left:1px solid #678FC8;
	border-top:1px solid #678FC8;
	border-right:1px solid #678FC8;
	border-bottom:1px solid #678FC8;
	padding-left:3px;
