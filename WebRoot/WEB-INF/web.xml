<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" version="2.4">
  <display-name>XJSCRM</display-name>
  <context-param>
    <param-name>ErrorRightPage</param-name>
    <param-value>/main/ErrorRightPage.jsp</param-value>
  </context-param>
  <filter>
    <filter-name>SetCharacterEncodingFilter</filter-name>
    <filter-class>com.gzunicorn.common.web.filters.SetCharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>GBK</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>LogonFilter</filter-name>
    <filter-class>com.gzunicorn.common.web.filters.LogonFilter</filter-class>
  </filter>
  <filter>
    <filter-name>JbpmContextFilter</filter-name>
    <filter-class>org.jbpm.web.JbpmContextFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SetCharacterEncodingFilter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SetCharacterEncodingFilter</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LogonFilter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>LogonFilter</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>JbpmContextFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.gzunicorn.common.web.listener.OnLineUserListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
    <init-param>
      <param-name>config</param-name>
      <param-value>/WEB-INF/xml/struts-config.xml,/WEB-INF/xml/struts-cb.xml,/WEB-INF/xml/struts-hqs.xml,/WEB-INF/xml/struts-cgw.xml,/WEB-INF/xml/struts-lyg.xml,/WEB-INF/xml/struts-lsw.xml</param-value>
    </init-param>
    <init-param>
      <param-name>debug</param-name>
      <param-value>3</param-value>
    </init-param>
    <init-param>
      <param-name>detail</param-name>
      <param-value>3</param-value>
    </init-param>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>QuartzInitializer</servlet-name>
    <servlet-class>org.quartz.ee.servlet.QuartzInitializerServlet</servlet-class>
    <init-param>
      <param-name>config-file</param-name>
      <param-value>/quartz.properties</param-value>
    </init-param>
    <init-param>
      <param-name>shutdown-on-unload</param-name>
      <param-value>true</param-value>
    </init-param>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ProcessImageServlet</servlet-name>
    <servlet-class>
			org.jbpm.webapp.servlet.ProcessImageServlet
		</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeployServlet</servlet-name>
    <servlet-class>
			org.jbpm.webapp.servlet.DeployServlet
		</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>
			org.jbpm.webapp.servlet.UploadServlet
		</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeployServlet</servlet-name>
    <url-pattern>/deploy</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProcessImageServlet</servlet-name>
    <url-pattern>/processimage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Jersey Web Application</servlet-name>
    <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Jersey Web Application</servlet-name>
    <url-pattern>/xj/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ContractPdfServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.ContractPdfServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContractPdfServlet</servlet-name>
    <url-pattern>/ContractPdfServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ConfigServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.ConfigServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DownLoadApk</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.DownLoadApk</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getImageServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.getImageServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PrintDisposeServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.PrintDisposeServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PrintCalloutProcessServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.PrintCalloutProcessServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PrintQualityCheckServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.PrintQualityCheckServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>PrintMaintenanceWorkPlanServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.PrintMaintenanceWorkPlanServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownLoadApk</servlet-name>
    <url-pattern>/DownLoadApkServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getImageServlet</servlet-name>
    <url-pattern>/getImageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrintDisposeServlet</servlet-name>
    <url-pattern>/PrintDisposeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrintCalloutProcessServlet</servlet-name>
    <url-pattern>/PrintCalloutProcessServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrintQualityCheckServlet</servlet-name>
    <url-pattern>/PrintQualityCheckServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PrintMaintenanceWorkPlanServlet</servlet-name>
    <url-pattern>/PrintMaintenanceWorkPlanServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/logon/logon.jsp</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <mime-mapping>
    <extension>xlsx</extension>
    <mime-type>application/vnd.openxmlformats-officedocument.spreadsheetml.sheet</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>docx</extension>
    <mime-type>application/vnd.openxmlformats-officedocument.wordprocessingml.document</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>xls</extension>
    <mime-type>application/octet-stream</mime-type>
  </mime-mapping>
  <mime-mapping>
    <extension>doc</extension>
    <mime-type>application/msword</mime-type>
  </mime-mapping>
  <servlet>
    <description></description>
    <display-name>PrintDebugFileServlet</display-name>
    <servlet-name>PrintDebugFileServlet</servlet-name>
    <servlet-class>com.gzunicorn.common.servlet.PrintDebugFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PrintDebugFileServlet</servlet-name>
    <url-pattern>/PrintDebugFileServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>DruidStatView</servlet-name>
     <servlet-class>com.alibaba.druid.support.http.StatViewServlet</servlet-class>
   </servlet>
   <servlet-mapping>
     <servlet-name>DruidStatView</servlet-name>
     <url-pattern>/druid/*</url-pattern>
   </servlet-mapping>
  <filter>  
  	<filter-name>DruidWebStatFilter</filter-name>  
  	<filter-class>com.alibaba.druid.support.http.WebStatFilter</filter-class>  
  	<init-param>  
      <param-name>exclusions</param-name>  
      <param-value>*.js,*.gif,*.jpg,*.png,*.css,*.ico,/druid/*</param-value>  
  	</init-param>
  	<init-param>
    	<param-name>profileEnable</param-name>
    	<param-value>true</param-value>
	</init-param>
	<init-param>
        <param-name>principalCookieName</param-name>
        <param-value>USER_COOKIE</param-value>
    </init-param>
    <init-param>
        <param-name>principalSessionName</param-name>
        <param-value>USER_SESSION</param-value>
    </init-param>  
  </filter>  
  <filter-mapping>  
  		<filter-name>DruidWebStatFilter</filter-name>  
  		<url-pattern>/*</url-pattern>  
  </filter-mapping>
  
</web-app>

